{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "Australia annual average Sunshine",
  "width": 800,
  "height": 400,
  "projection": { "type": "equalEarth" },
  "data": {
    "url": "https://raw.githubusercontent.com/KokHeeTan/fit3179-testrepo/refs/heads/main/Week9_Homework/js/ne_DV2.topojson",
    "format": { "type": "topojson", "feature": "states" }
  },
  "transform": [
    {
      "lookup": "properties.NAME",
      "from": {
        "data": { "url": "https://raw.githubusercontent.com/KokHeeTan/fit3179-testrepo/refs/heads/main/Week9_Homework/sunshine_grouped_states.csv" },
        "key": "state",
        "fields": ["sunshine_hour"]
      }
    }
  ],
  "mark": { "type": "geoshape" },
  "encoding": {
    "color": {
      "field": "sunshine_hour",
      "type": "quantitative",
      "title": "Sunshine hour",
      "scale": { "scheme": "yelloworangered", "domain": [0, 10] }
    },
    "tooltip": [
      { "field": "properties.NAME", "type": "nominal", "title": "state" },
      { "field": "sunshine_hour", "type": "quantitative" }
    ]
  },
  "config": { "view": { "stroke": null }, "background": "#ffffff" }
}
